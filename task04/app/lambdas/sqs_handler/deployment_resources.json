{
  "sqs_handler-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      {
        "policy_name": "lambda-basic-execution",
        "policy_document": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sqs:GetQueueAttributes"
              ],
              "Resource": "*"
            }
          ]
        }
      }
    ],
    "resource_type": "iam_role",
    "tags": {}
  }
}